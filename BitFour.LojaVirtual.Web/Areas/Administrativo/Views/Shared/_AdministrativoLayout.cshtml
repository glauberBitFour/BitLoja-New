<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width">
    <title>@ViewBag.Title - BitLoja Administrativo</title>
    <link href="~/Content/bootstrap-theme.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Error.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <!---->


    <script src="~/Scripts/jQueryFixes.js"></script>
    <!--Para utlizar o jQuery.Unobtrusive.validation na classe alterar para validar o preço, é preciso referenciar o mesmo nesta pagina-->
    
   
</head>
<body>
    <div>
        @if (TempData["mensagem"]!=null)    
        {
            <!--recebe a mensagem de alteraçao da produto controller-->
            <div class="alert alert-success" id="mensagem" >@TempData["mensagem"]</div>
        }
        @RenderBody()
    </div>
</body>
</html>
